<form class="d-inline" method="post" action="{{ path('solicitud_delete', {'id': solicitud.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ solicitud.id) }}">
    <button class="btn btn-danger">Borrar</button>
</form>
